   <!-- actions opening views on models -->
   
    <record model="ir.actions.act_window" id="bissweb.dataopen_action_window">
        <field name="name">Data mở(Lead)</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">crm.lead</field>
        <field name="view_mode">tree,pivot,graph,form</field>
        <field name="domain">[('type','=','lead'),('user_id','=',False)]</field> 
        <field name="context">{
                'default_type': 'lead',
                'search_default_type': 'lead',
            }</field>
    </record>

    <record id="dataopen_action_view_tree" model="ir.actions.act_window.view">
            <field name="sequence" eval="1"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="dataopen_view_tree"/>
            <field name="act_window_id" ref="bissweb.dataopen_action_window"/>
    </record>

