<!-- Top menu item -->
    <menuitem
        id="bissweb.crm_menu_dataopen"
        name="Leads mở"
        parent="crm.crm_menu_root"
        action="bissweb.dataopen_action_window"
        groups="bissweb.group_crm_teamsale_user"
        sequence="10"/>
  </data>
